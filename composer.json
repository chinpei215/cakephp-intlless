{
    "name": "chinpei215/cakephp-intlless",
    "description": "Intlless plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Intlless\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Intlless\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "config": {
        "platform": {
            "ext-intl": "0.0.0"
        }
    }
}
